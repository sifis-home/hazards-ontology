    <!-- https://purl.org/sifis/hazards#hasCategory -->

    <owl:ObjectProperty rdf:about="https://purl.org/sifis/hazards#hasCategory">
        <rdfs:domain rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <rdfs:range rdf:resource="https://purl.org/sifis/hazards#Category"/>
        <schema:domainIncludes rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <schema:rangeIncludes rdf:resource="https://purl.org/sifis/hazards#Category"/>
        <rdfs:comment xml:lang="en">A relationship identifying the category of a hazard.</rdfs:comment>
        <rdfs:label xml:lang="en">has category</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/sifis/hazards#hasHazard -->

    <owl:ObjectProperty rdf:about="https://purl.org/sifis/hazards#hasHazard">
        <rdfs:domain rdf:resource="https://www.w3.org/2019/wot/td#InteractionAffordance"/>
        <rdfs:range rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <schema:domainIncludes rdf:resource="https://www.w3.org/2019/wot/td#InteractionAffordance"/>
        <schema:rangeIncludes rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <rdfs:comment xml:lang="en">A relationship identifying a hazard of an interaction affordance, being either a property, an action, or an event.</rdfs:comment>
        <rdfs:label xml:lang="en">has hazard</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
