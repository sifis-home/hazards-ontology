    <!-- https://purl.org/sifis/hazards#Category -->

    <owl:Class rdf:about="https://purl.org/sifis/hazards#Category">
        <owl:disjointWith rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <owl:disjointWith rdf:resource="https://www.w3.org/2019/wot/td#InteractionAffordance"/>
        <rdfs:comment xml:lang="en">A broad class gathering hazards with similar characteristics.</rdfs:comment>
        <rdfs:label xml:lang="en">Category</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/sifis/hazards#Hazard -->

    <owl:Class rdf:about="https://purl.org/sifis/hazards#Hazard">
        <owl:disjointWith rdf:resource="https://www.w3.org/2019/wot/td#InteractionAffordance"/>
        <rdfs:comment xml:lang="en">A possible risk associated with the invocation of a given interaction affordance. Within a thing description, properties, actions, and events can be invoked, and their execution may lead to hazards.</rdfs:comment>
        <rdfs:label xml:lang="en">Hazard</rdfs:label>
    </owl:Class>
    


    <!-- https://www.w3.org/2019/wot/td#InteractionAffordance -->

    <owl:Class rdf:about="https://www.w3.org/2019/wot/td#InteractionAffordance">
        <rdfs:comment xml:lang="en">Metadata of a Thing that defines how a consumer may interact with the Thing.</rdfs:comment>
        <rdfs:label xml:lang="en">Interaction affordance</rdfs:label>
    </owl:Class>
    


