     -->

    


    <!-- https://purl.org/sifis/hazards#description -->

    <owl:DatatypeProperty rdf:about="https://purl.org/sifis/hazards#description">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/sifis/hazards#Category"/>
                    <rdf:Description rdf:about="https://purl.org/sifis/hazards#Hazard"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <schema:domainIncludes rdf:resource="https://purl.org/sifis/hazards#Category"/>
        <schema:domainIncludes rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <schema:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A relationship identifying the description of either a hazard or a category.</rdfs:comment>
        <rdfs:label xml:lang="en">description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/sifis/hazards#name -->

    <owl:DatatypeProperty rdf:about="https://purl.org/sifis/hazards#name">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/sifis/hazards#Category"/>
                    <rdf:Description rdf:about="https://purl.org/sifis/hazards#Hazard"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <schema:domainIncludes rdf:resource="https://purl.org/sifis/hazards#Category"/>
        <schema:domainIncludes rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <schema:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A relationship identifying the name of either a hazard or a category.</rdfs:comment>
        <rdfs:label xml:lang="en">name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://purl.org/sifis/hazards#riskScore -->

    <owl:DatatypeProperty rdf:about="https://purl.org/sifis/hazards#riskScore">
        <rdfs:domain rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <rdfs:range rdf:resource="https://purl.org/sifis/hazards#level"/>
        <schema:domainIncludes rdf:resource="https://purl.org/sifis/hazards#Hazard"/>
        <schema:rangeIncludes rdf:resource="https://purl.org/sifis/hazards#level"/>
        <rdfs:comment xml:lang="en">A relationship identifying the risk score associated with a hazard.</rdfs:comment>
        <rdfs:label xml:lang="en">risk score</rdfs:label>
    </owl:DatatypeProperty>
    


